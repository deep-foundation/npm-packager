{
  "name": "@deep-foundation/npm-packager",
  "version": "0.0.12",
  "license": "Unlicense",
  "keywords": [
    "deep-package"
  ],
  "scripts": {
    "build": "npx ts-node build.ts",
    "import": "npm run build && npx ts-node import.ts",
    "test-install": "npx ts-node test-install.ts",
    "test-publish": "npx ts-node test-publish.ts",
    "test-deep": "npx ts-node test-deep.ts",
    "publish-next-version": "npm run build; npm run test; export CURRENT_VERSION=$(npx ts-node set-next-version.ts); git add .; git commit -m $CURRENT_VERSION; git push",
    "test": "jest"
  },
  "dependencies": {
    "@deep-foundation/core": "^0.0.1"
  },
  "devDependencies": {
    "@deep-foundation/deeplinks": "0.0.98",
    "@deep-foundation/hasura": "0.0.20",
    "@types/jest": "^29.4.0",
    "jest": "^29.5.0",
    "semver": "^7.3.8",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}