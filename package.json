    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "semver": "^7.3.8",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
